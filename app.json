{
  "name": "BodyBuilder",
  "displayName": "BodyBuilder",
  "expo": {
    "name": "BodyBuilder",
    "slug": "bodybuilder",
    "scheme": "BodyBuilder",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/pen-logo.png",
    "splash": {
      "image": "./assets/shadow-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF"
    },
    "userInterfaceStyle": "automatic",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "icon": "./assets/pen-logo.png",
      "package": "com.gymwork",
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./assets/pen-logo.png",
        "backgroundImage": "./assets/images/app-icon-android-adaptive-background.png"
      },
      "splash": {
        "image": "./assets/shadow-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFF"
      }
    },
    "ios": {
      "icon": "./assets/pen-logo.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.gymwork",
      "splash": {
        "image": "./assets/shadow-icon.png",
        "tabletImage": "./assets/shadow-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFF"
      }
    },
    "web": {
      "favicon": "./assets/shadow-icon.png",
      "splash": {
        "image": "./assets/shadow-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFF"
      },
      "bundler": "metro"
    },
    "plugins": [
      "expo-localization",
      "expo-font",
      "expo-build-properties"
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "0b395f1b-f91b-48a2-aad5-70036988be1b"
      }
    },
    "owner": "peter.lazarov94"
  },
  "ignite": {
    "version": "9.8.0"
  }
}
