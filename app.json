{
  "name": "Gymwork",
  "displayName": "Gymwork",
  "expo": {
    "name": "Gymwork",
    "slug": "Gymwork",
    "scheme": "gymwork",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pen-logo.png",
    "splash": {
      "image": "./assets/shadow-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF"
    },
    "userInterfaceStyle": "automatic",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": ["**/*"],
    "android": {
      "icon": "./assets/pen-logo.png",
      "package": "com.gymwork",
      "adaptiveIcon": {
        "foregroundImage": "./assets/pen-logo.png",
        "backgroundImage": "./assets/images/app-icon-android-adaptive-background.png"
      },
      "splash": {
        "image": "./assets/shadow-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFF"
      }
    },
    "ios": {
      "icon": "./assets/pen-logo.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.gymwork",
      "splash": {
        "image": "./assets/shadow-icon.png",
        "tabletImage": "./assets/shadow-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFF"
      }
    },
    "web": {
      "favicon": "./assets/shadow-icon.png",
      "splash": {
        "image": "./assets/shadow-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFF"
      },
      "bundler": "metro"
    },
    "plugins": ["expo-localization", "expo-font", "expo-build-properties"],
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "f170b951-e464-4e11-a6df-5af2a8dcd8c3"
      }
    },
    "owner": "peter.lazarov94"
  },
  "ignite": {
    "version": "9.8.0"
  }
}
